        android:alpha="0.8"
        android:background="@drawable/nav_navballbackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline5"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/toggleButtonRCS"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:alpha="0.8"
        android:background="@drawable/nav_rcsbutton"
        android:checked="true"
        android:text="ToggleButton"
        android:textOff="RCS"
        android:textOn="RCS"
        app:layout_constraintBottom_toBottomOf="@+id/buttonVelocity"
        app:layout_constraintEnd_toStartOf="@+id/guideline5"
        app:layout_constraintStart_toEndOf="@+id/buttonVelocity"
        app:layout_constraintTop_toTopOf="@+id/buttonVelocity"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonVelocity"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:alpha="0.8"
        android:background="@drawable/nav_velbutton"
        android:text="Orbit\n234.7m/s"
        android:textAllCaps="false"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/imageViewNavball"
        app:layout_constraintStart_toStartOf="@+id/imageViewNavball"
        app:layout_constraintTop_toBottomOf="@+id/buttonAltitude" />

    <ImageView
        android:id="@+id/imageViewAltitude"
        android:layout_width="30dp"
        android:layout_height="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonAltitude"
        app:layout_constraintEnd_toStartOf="@+id/buttonAltitude"
        app:layout_constraintStart_toStartOf="@+id/toggleButtonSAS"
        app:layout_constraintTop_toTopOf="@+id/buttonAltitude"
        app:srcCompat="@drawable/nav_altsea" />

    <Button
        android:id="@+id/buttonAltitude"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:alpha="0.8"
        android:background="@drawable/nav_altbutton"
        android:text="65234m"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/toggleButtonRCS"
        app:layout_constraintStart_toStartOf="@+id/buttonVelocity"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonTarget"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:alpha="0.8"
        android:background="@drawable/nav_tgtbutton"
        android:text="123m/s\n2349m"
        android:textAllCaps="false"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonManeuver"
        app:layout_constraintEnd_toStartOf="@+id/guideline5"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/buttonManeuver" />

    <Button
        android:id="@+id/buttonManeuver"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="8dp"
        android:alpha="0.8"
        android:background="@drawable/nav_mnnbutton"
        android:text="435m/s\nT-43s"
        android:textAllCaps="false"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonStage"
        app:layout_constraintEnd_toStartOf="@+id/buttonTarget"
        app:layout_constraintStart_toStartOf="@+id/guideline4" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <Button
        android:id="@+id/buttonAbort"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:alpha="0.8"
        android:background="@drawable/nav_abtbutton"
        android:text="!"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonStage"
        app:layout_constraintEnd_toEndOf="@+id/buttonTarget"
        app:layout_constraintTop_toTopOf="@+id/buttonStage" />

    <Button
        android:id="@+id/buttonStage"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:alpha="0.8"
        android:background="@drawable/nav_stgbutton"
        android:text="STAGE"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonAbort"
        app:layout_constraintStart_toStartOf="@+id/buttonManeuver" />

    <ToggleButton
        android:id="@+id/toggleButtonAntiTarget"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/vect_sasselect"
        android:checked="false"
        android:foreground="@drawable/vect_targetrbutton"
        android:text="ToggleButton"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintEnd_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/toggleButtonRadialOut" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <ToggleButton
        android:id="@+id/toggleButtonAG3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/act_actionbutton_back"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="ToggleButton"
        android:textOff="Action 3"
        android:textOn="Action 3"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/toggleButtonRadialIn"
        app:layout_constraintEnd_toStartOf="@+id/toggleButtonRadialIn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toggleButtonRadialIn" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.17" />

    <ToggleButton
        android:id="@+id/toggleButtonLight"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:alpha="0.8"
        android:background="@drawable/act_button_back"
        android:foreground="@drawable/act_lightbutton"
        android:text="ToggleButton"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="@+id/buttonStage"
        app:layout_constraintStart_toStartOf="@+id/toggleButtonAG5" />

    <Button
        android:id="@+id/buttonPower"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:alpha="0.8"
        android:background="@drawable/menu_button_back_red"
        android:foreground="@drawable/menu_powerbutton"
        app:layout_constraintBottom_toBottomOf="@+id/buttonAltitude"
        app:layout_constraintStart_toStartOf="@+id/toggleButtonAG1"
        app:layout_constraintTop_toTopOf="@+id/buttonAltitude" />

    <ImageView
        android:id="@+id/imageViewOX"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fuel_background"
        android:foreground="@drawable/fuel_background"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:srcCompat="@android:color/transparent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.83" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OX"
        android:textColor="#aaaaaa"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonAbort"
        app:layout_constraintEnd_toEndOf="@+id/imageViewOX"
        app:layout_constraintStart_toStartOf="@+id/imageViewOX" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/imageViewES"
        app:layout_constraintStart_toStartOf="@+id/imageViewES"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/electricity" />

    <TextView
        android:id="@+id/textViewStatus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:gravity="center"
        android:text="Waiting for broadcast..."
        android:textColor="#aaaaaa"
        app:layout_constraintBottom_toTopOf="@+id/buttonTarget"
        app:layout_constraintEnd_toEndOf="@+id/buttonTarget"
        app:layout_constraintStart_toStartOf="@+id/buttonManeuver" />

    <Button
        android:id="@+id/buttonConnect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu_button_back_gray"
        android:text="Connect"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

</android.support.constraint.ConstraintLayout>
